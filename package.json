{
  "name": "ves-cli",
  "version": "0.1.0",
  "description": "ves framework cli",
  "bin": {
    "ves": "bin/cli.js"
  },
  "keywords": [
    "npm",
    "npm package",
    "unit test",
    "eslint",
    "travis",
    "travis-ci",
    "code coverage",
    "changelog"
  ],
  "dependencies": {
    "@types/node": "^10.12.0",
    "easywebpack-cli": "next",
    "easywebpack-vue": "next",
    "egg-bin": "^4.9.0",
    "egg-scripts": "^2.10.0",
    "egg-ts-helper": "^1.13.0",
    "node-tool-utils": "^1.1.1",
    "ts-loader": "^4.0.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.6.2",
    "webpack-merge": "^4.1.4"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "codecov": "^3.0.0",
    "conventional-changelog-cli": "^1.3.5",
    "cross-env": "^5.0.5",
    "eslint": "^4.5.0",
    "eslint-config-eslint": "^4.0.0",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "test": "nyc --reporter=html --reporter=text mocha --timeout=1000000",
    "cov": "nyc report --reporter=lcov && codecov --token=e971d1e7-39e2-44b1-b1ee-f4345b0adbee",
    "ci": "npm run lint && npm run cov",
    "ii": "npm install --registry https://registry.npm.taobao.org"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "test/*.test.js"
    ]
  },
  "ci": {
    "version": "8, 10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ves-team/ves-cli.git"
  },
  "bugs": {
    "url": "https://github.com/ves-team/ves-cli/issues"
  },
  "homepage": "https://github.com/ves-team/ves-cli#readme",
  "author": "hubcarl@126.com",
  "license": "MIT"
}
